{
  "name": "webpack-with-electron-sample",
  "version": "1.0.0",
  "main": "build/main",
  "scripts": {
    "dev-electron": "wait-on build/main && wait-on build/renderer && electronmon .",
    "dev-main": "webpack serve --config webpack.main.config.js --mode=development",
    "dev-renderer": "webpack serve --config webpack.renderer.config.js --mode=development",
    "dev": "concurrently \"npm run dev-main\" \"npm run dev-renderer\" \"npm run dev-electron\""
  },
