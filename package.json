    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "electron": "^34.0.1",
    "electronmon": "^2.0.3",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "wait-on": "^8.0.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
